# ============================================
# {{APP_NAME}} Environment Configuration
# ============================================
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control
# ============================================

# RevenueCat Configuration (REQUIRED)
# Get your API keys from https://app.revenuecat.com/projects
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY=appl_YOUR_IOS_KEY_HERE
EXPO_PUBLIC_REVENUECAT_ANDROID_API_KEY=goog_YOUR_ANDROID_KEY_HERE

# App Configuration
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_PRIVACY_POLICY_URL=https://example.com/privacy
EXPO_PUBLIC_TERMS_OF_SERVICE_URL=https://example.com/terms
EXPO_PUBLIC_SUPPORT_EMAIL=support@example.com

# Analytics - PostHog (OPTIONAL)
# Get your API key from https://posthog.com (Project Settings)
# Free tier: 1M events/month
# EXPO_PUBLIC_POSTHOG_API_KEY=phc_YOUR_KEY_HERE
# EXPO_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Error Tracking - Sentry (OPTIONAL)
# Get your DSN from https://sentry.io
# EXPO_PUBLIC_SENTRY_DSN=https://xxx@sentry.io/xxx

# Environment
EXPO_PUBLIC_ENV=development
