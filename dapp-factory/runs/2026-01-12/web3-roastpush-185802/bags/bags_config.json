{
  "token_creation": {
    "metadata": {
      "name": "RoastPush",
      "symbol": "ROAST",
      "description": "ROAST is the native token for RoastPush, a competitive roast battle platform where users compete in voice-first battles for token rewards. Entry fees, prizes, tips, and cosmetics all flow through ROAST.",
      "decimals": 9
    },
    "launch_params": {
      "initialBuyAmount": "100000000",
      "totalSupply": "1000000000000000000"
    }
  },
  "fee_routing": {
    "creator_share": 0.75,
    "partner_share": 0.25,
    "partner_key": "FDYcVLxHkekUFz4M29hCuBH3vbf1aLm62GEFZxLFdGE7",
    "immutable": true
  },
  "fee_share_v2_config": {
    "feeShareConfig": {
      "feeClaimers": [
        {
          "wallet": "${CREATOR_WALLET}",
          "bps": 7500
        },
        {
          "wallet": "FDYcVLxHkekUFz4M29hCuBH3vbf1aLm62GEFZxLFdGE7",
          "bps": 2500
        }
      ],
      "partnerKey": "FDYcVLxHkekUFz4M29hCuBH3vbf1aLm62GEFZxLFdGE7"
    }
  },
  "sdk_integration": {
    "package": "@bags/sdk",
    "api_base": "https://public-api-v2.bags.fm/api/v1/",
    "methods_used": [
      "createTokenInfoAndMetadata",
      "createLaunchTransaction",
      "uploadFile"
    ]
  },
  "environment_variables": {
    "BAGS_API_KEY": {
      "description": "Bags API key for authenticated requests",
      "required": true,
      "source": "process.env.BAGS_API_KEY"
    },
    "SOLANA_RPC_URL": {
      "description": "Solana RPC endpoint URL",
      "required": true,
      "source": "process.env.SOLANA_RPC_URL"
    },
    "CREATOR_WALLET": {
      "description": "Creator wallet address for fee routing",
      "required": true,
      "source": "process.env.CREATOR_WALLET"
    },
    "NETWORK": {
      "description": "Solana network (mainnet-beta or devnet)",
      "required": true,
      "source": "process.env.NETWORK",
      "default": "mainnet-beta"
    }
  },
  "validation": {
    "partner_key_hardcoded": true,
    "fee_split_correct": true,
    "env_vars_only": true,
    "deterministic": true
  }
}
