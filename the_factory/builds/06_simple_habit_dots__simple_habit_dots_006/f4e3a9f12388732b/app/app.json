{
  "expo": {
    "name": "Simple Habit Dots",
    "slug": "simple-habit-dots",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8fafc"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simplehabitdots.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized habit insights and improve your experience."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f8fafc"
      },
      "package": "com.simplehabitdots.app",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "scheme": "simple-habit-dots",
    "plugins": [
      "expo-router",
      [
        "react-native-purchases",
        {
          "enableAmazonAppstore": false
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "placeholder-eas-project-id"
      }
    }
  }
}