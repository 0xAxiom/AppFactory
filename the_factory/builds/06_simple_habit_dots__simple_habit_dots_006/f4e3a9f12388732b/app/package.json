{
  "name": "simple-habit-dots",
  "version": "1.0.0",
  "description": "Visual habit tracking with colored dots - unlimited and simple",
  "main": "App.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android", 
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android"
  },
  "dependencies": {
    "expo": "~50.0.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "expo-router": "^3.0.0",
    "react-native-purchases": "^7.0.0",
    "expo-sqlite": "~13.4.0",
    "react-native-elements": "^3.4.3",
    "@react-navigation/native": "^6.1.0",
    "expo-dev-client": "~3.3.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "expo-linear-gradient": "~12.7.0",
    "expo-constants": "~15.4.0",
    "expo-linking": "~6.2.2",
    "expo-status-bar": "~1.11.1",
    "react-native-vector-icons": "^10.0.0",
    "expo-haptics": "~12.8.1",
    "expo-system-ui": "~2.9.0",
    "@expo/vector-icons": "^13.0.0",
    "react-native-super-grid": "^4.9.5",
    "expo-calendar": "~12.8.0",
    "date-fns": "^3.0.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "~7.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "habit tracker",
    "visual tracking", 
    "streaks",
    "dots",
    "unlimited habits",
    "simple productivity",
    "expo",
    "react native"
  ],
  "author": "Simple Habit Dots Team",
  "license": "MIT",
  "private": true,
  "expo": {
    "install": {
      "exclude": ["react-native-safe-area-context"]
    }
  }
}