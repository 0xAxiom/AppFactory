{
  "expo_sdk_version": "52",
  "react_native_version": "0.76.x",
  "dependencies": {
    "expo": "~52.0.0",
    "expo-router": "~4.0.0",
    "react": "18.3.1",
    "react-native": "0.76.x",
    "expo-av": "~15.0.0",
    "@siteed/expo-audio-studio": "^1.0.0",
    "react-native-audio-waveform": "^1.0.0",
    "expo-file-system": "~18.0.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "react-native-purchases": "^8.0.0",
    "react-native-svg": "^15.0.0",
    "react-native-reanimated": "~3.16.0",
    "react-native-gesture-handler": "~2.20.0",
    "expo-haptics": "~14.0.0",
    "expo-linear-gradient": "~14.0.0",
    "expo-location": "~18.0.0",
    "expo-sharing": "~13.0.0",
    "uuid": "^10.0.0",
    "date-fns": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "~5.3.0",
    "@types/react": "~18.2.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "install_commands": [
    "npx create-expo-app@latest evp-analyzer --template tabs",
    "npx expo install expo-av expo-file-system expo-haptics expo-linear-gradient expo-location expo-sharing react-native-svg react-native-reanimated react-native-gesture-handler @react-native-async-storage/async-storage",
    "npm install react-native-purchases @siteed/expo-audio-studio react-native-audio-waveform uuid date-fns"
  ],
  "configuration_required": {
    "react-native-purchases": {
      "ios_api_key": "REVENUECAT_API_KEY_IOS",
      "android_api_key": "REVENUECAT_API_KEY_ANDROID",
      "setup_url": "https://docs.revenuecat.com/docs/getting-started"
    },
    "expo-av": {
      "ios_background_modes": ["audio"],
      "app_json_config": {
        "expo": {
          "ios": {
            "infoPlist": {
              "UIBackgroundModes": ["audio"]
            }
          }
        }
      }
    }
  }
}
