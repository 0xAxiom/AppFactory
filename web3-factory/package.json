{
  "name": "web3-factory",
  "version": "1.0.0",
  "description": "Solana-first Web3 app factory with Bags SDK integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "web3": "./dist/web3factory.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "web3": "tsx web3factory.ts",
    "generate": "tsx generator/index.ts",
    "validate": "tsx validator/index.ts",
    "zip": "tsx validator/zip.ts",
    "launch-token": "tsx scripts/launch_token.ts",
    "launch-token-v2": "tsx scripts/launch_token_v2.ts",
    "validate-env": "tsx scripts/validate_environment.ts",
    "test-token-free": "tsx scripts/test_token_free_flow.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "web3",
    "bags",
    "token-launch",
    "dapp"
  ],
  "author": "App Factory",
  "license": "MIT",
  "dependencies": {
    "@bagsfm/bags-sdk": "latest",
    "@solana/web3.js": "^1.87.0",
    "dotenv": "^16.3.1",
    "bs58": "^5.0.0",
    "handlebars": "^4.7.8",
    "archiver": "^6.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/archiver": "^6.0.0",
    "typescript": "^5.2.0",
    "tsx": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}